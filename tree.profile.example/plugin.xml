<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.papyrus.uml.extensionpoints.UMLProfile">
      <profile
            description="My Tree Profile"
            name="TreeProfile"
            path="pathmap://MYTREE/tree.profile.example.profile.uml">
      </profile>
   </extension>

   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="pathmap://MYTREE/"
            target="platform:/plugin/tree.profile.example/">
      </mapping>
   </extension>
   


</plugin>
